<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
     >

    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:gravity="center"
        android:layout_marginBottom="5dp">
            
   	    <ImageView
     		android:id="@+id/imageView1"
       		android:layout_width="50dp"
        	android:layout_height="50dp"
        	android:layout_marginTop="24dp"
        	android:contentDescription="@string/iv_UserProfile_Image_contentdescription"
        	android:src="@drawable/ic_profile" />
    
    	<TextView
        	android:id="@+id/etuserProfilename"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
       	 	android:ems="10"
        	android:text="@string/et_userName" 
        	android:gravity="center"
        	android:textSize="14sp"/>
    
    	<TextView
       	 	android:id="@+id/etuserProfileProfession"
        	android:layout_width="wrap_content"
        	android:layout_height="wrap_content"
        	android:ems="10"
        	android:text="@string/et_User_Profession"
        	android:gravity="center"
        	android:textSize="13sp"/>

     	<TextView
         	android:id="@+id/etuserProfileLocation"
         	android:layout_width="wrap_content"
         	android:layout_height="wrap_content"
         	android:ems="10"
         	android:gravity="center"
         	android:text="@string/et_user_location"
         	android:textSize="12sp" />

    </LinearLayout>
    
    <View
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="#c0c0c0"/>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileEmaillabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:layout_gravity="right"
            
            android:text="@string/tv_userProfileEmailLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileEmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:text="@string/tv_userProfileEmailLabel"
            android:textSize="12sp" />
        
    </LinearLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfilePhoneLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userProfilePhoneLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfilePhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/hint_phonenumber"
            android:textSize="12sp" />
        
    </LinearLayout>
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileDOBLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:hint="@string/hint_dob"
            android:text="@string/tv_userProfileDOBLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileDOB"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:text="@string/hint_dob"
            android:textSize="12sp" />
        
    </LinearLayout>
        <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileWeightLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userWeightLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileWeight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userWeightLabel"
            android:textSize="12sp" />
        
    </LinearLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileHeightLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userHeightLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileHeight"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userHeightLabel"
            android:textSize="12sp" />
        
    </LinearLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileDietHabitLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userDietHabitLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileDietHabit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userDietHabitLabel"
            android:textSize="12sp" />
        
    </LinearLayout>
    
    <View
        android:layout_width="fill_parent"
        android:layout_height="2dp"
        android:background="#c0c0c0"/>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp"
        android:layout_marginTop="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileDailyScheduleLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userDailyScheduleLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileDailySchedule"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userDailyScheduleLabel"
            android:textSize="12sp" />
        
    </LinearLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileTargetLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userProfileTargetLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileTarget"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_usertargetgoal"
            android:textSize="12sp" />
        
    </LinearLayout>
    
    <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileTargetDateLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tv_userProfileTargetDateLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileDate"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/hint_dob"
            android:textSize="12sp" />
        
    </LinearLayout>
        <LinearLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginBottom="5dp">
 
        <TextView
            android:id="@+id/tvuserProfileAmountTimeSpentLabel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tvuserProfileAmountTimeSpentLabel"
            android:textSize="12sp" />
        
        <TextView
            android:id="@+id/tvuserProfileAmountTimeSpent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="@string/tvTimeSpend"
            android:textSize="12sp"/>

        <TextView
            android:id="@+id/tvuserProfileperweek"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:ems="10"
            android:text="per week"
            android:textSize="12sp" />
        
    </LinearLayout>

    
</LinearLayout>

